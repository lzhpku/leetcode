            if(i >= 5 && x[i - 2] >= x[i - 4] && x[i - 1] <= x[i - 3] && x[i] + x[i - 4] >= x[i - 2] && x[i - 1] + x[i - 5] >= x[i - 3])\
                return true;\
        \}\
        return false;\
    \}\
