public:\
    bool isSelfCrossing(vector<int>& x) \{\
        for(int i = 3; i < x.size(); i ++) \{\
            if(x[i] >= x[i - 2] && x[i - 1] <= x[i - 3])\
                return true;\
            if(i >= 4 && x[i] + x[i - 4] >= x[i - 2] && x[i - 1] == x[i - 3])\
